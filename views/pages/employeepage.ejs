        <p class="rectangle-one"><%= users.email %></p>
    </div>
  
    <div>
        <h2>Your personal schedule</h2>
  
        <div>
            <table>
                <tr>
                    <th>Day </th>
                    <th>Start ðŸ•‘</th>
                    <th>End ðŸ•‘</th>
                </tr>

                <% for(let i =0; i < mySchedule.length; i++) { %>
                    <tb>
                        <tr><%= mySchedule[i].day %></tr>
                        <tr><%= mySchedule[i].start_time %></tr>
                        <tr><%= mySchedule[i].end_time %></tr>
                    </tb>
                <%}%>
            </table>
      </div>
    </div>
</section>
  


