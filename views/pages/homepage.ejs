<div>
    <h2>All your lovely people's schedule <span>&#128520;</span></h2>

    <div class="home-wrapper">
        <table>
            <tr>
                <th>Personal ID</th>
                <th>Name</th>
                <th>Day</th>
                <th>Start Time</th>
                <th>End Time</th>
            </tr>

            <% for(let i = 0; i < schedule.length; i++) { %>
                <tr>
                  <td><%= schedule[i].user_id %></td>
                  <td><%= schedule[i].username %></td>
                  <td><%= schedule[i].day %></td>
                  <td><%= schedule[i].start_time %></td>
                  <td><%= schedule[i].end_time %></td>
                </tr>
                <% } %>
        </table>
    </div>
</div>
